<div class="container usershowpage">
<br>
 <div class="row">
  <% users.forEach(user => { %>
    <div class="col-md-3 col-lg-2 col-sm-4 col-xs-12">
      <a id="usrlnk" href="/users/<%= user.id %>">
      <div id="usrimg">
         <img id="profile" src="<%= user.photo %>">
      </div>
      <div id="name"><p id="usrnm"><%= user.firstName %></p></div>
      </a>
    </div>
  <% }) %>

  </div>
</div>
